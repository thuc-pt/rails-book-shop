<!DOCTYPE html>
<html>
  <head>
    <title>Libook</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag 'homes' %>
    <%= stylesheet_link_tag 'owl.carousel.min' %>
    <%= stylesheet_link_tag 'owl.theme.default.min' %>
    <%= stylesheet_link_tag 'admin' %>
    <%= stylesheet_link_tag 'carts' %>
    <%= stylesheet_link_tag 'animate' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'wow.min' %>
    <script>
        new WOW().init();
    </script>
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.2/css/all.css">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>
    <%= javascript_include_tag 'owl.carousel.min' %>
  </head>

  <body>

    <%=render 'layouts/header' %>

    <div id="content">
      <%= yield %>
    </div>

    <%=render 'layouts/footer' %>
  </body>

</html>